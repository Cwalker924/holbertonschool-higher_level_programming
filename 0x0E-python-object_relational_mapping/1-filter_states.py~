#!/usr/bin/python3
#
import sys
import MySQLdb

usa_db = MySQLdb.connect(user=sys.argv[1], passwd=sys.argv[2], db=sys.argv[3])
cursor = usa_db.cursor()
cursor.execute("SELECT * From states ORDER BY id ASC;")
states = cursor.fetchall()

for state in states:
    if state[1][0] is "N":
        print(state)

cursor.close()
usa_db.close()
